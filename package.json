{
  "name": "leczo",
  "version": "0.1.0",
  "description": "Experimental light-weight rendering library",
  "source": "src/index.ts",
  "main": "dist/leczo.js",
  "module": "dist/leczo.module.js",
  "umd:main": "dist/leczo.umd.js",
  "unpkg": "dist/leczo.min.js",
